    "pre-commit": "lint-staged",
    "graphql:codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "graphql:codegen:watch": "graphql-codegen --config codegen.yml -r dotenv/config --watch"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@reduxjs/toolkit": "^1.5.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.9.2",
    "dayjs": "^1.10.4",
    "dotenv": "^9.0.0",
    "graphql": "^15.5.0",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.21",
    "next": "10.0.3",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^8.2.0",
    "nprogress": "^0.2.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.16.0",
    "react-redux": "^7.2.4",
    "styled-components": "^5.3.0"
