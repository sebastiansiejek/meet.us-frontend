{
  "name": "meet.us",
  "version": "1.0.0",
  "scripts": {
    "dev": "yarn graphql:codegen && next -p 3001",
    "build": "next build",
    "start": "next start -p $PORT",
    "analyze": "ANALYZE=true next build",
    "type-check": "tsc",
    "pre-commit": "lint-staged",
    "format": "prettier . --write '!.next/**/*'",
    "lint": "eslint --fix",
    "graphql:codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "graphql:codegen:watch": "graphql-codegen --config codegen.yml -r dotenv/config --watch",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3001 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3001 cypress:headless"
  },
  "engines": {
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@graphql-codegen/typescript-resolvers": "^2.3.2",
    "@next/bundle-analyzer": "^11.1.2",
    "@splidejs/react-splide": "^0.6.17",
    "@types/randomcolor": "^0.5.6",
    "antd": "^4.18.3",
    "calendar-link": "^2.1.1",
    "chart.js": "^3.7.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "graphql": "^15.6.1",
    "gsap": "^3.8.0",
    "hamburger-react": "^2.4.1",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "next": "^11.1.2",
    "next-auth": "^4.0.6",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^8.9.0",
    "next-plugin-antd-less": "^1.4.4",
    "nprogress": "^0.2.0",
    "prettier": "^2.4.1",
    "randomcolor": "^0.6.2",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-leaflet": "^3.2.2",
    "react-leaflet-markercluster": "^3.0.0-rc1",
    "react-loading-skeleton": "^3.0.1",
    "react-query": "^3.28.0",
    "redaxios": "^0.4.1",
    "styled-components": "^5.3.3",
    "yarn-upgrade-all": "^0.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@graphql-codegen/typescript-react-query": "^3.5.2",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/js-cookie": "^3.0.0",
    "@types/leaflet": "^1.7.6",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.5",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.10",
    "@types/styled-components": "^5.1.15",
    "autoprefixer": "^10.3.7",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "cypress": "^9.2.1",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.7",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.4",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.4.4"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "yarn run lint",
    "*.{ts,tsx,js,jsx,css,md}": "yarn run format"
  }
}
