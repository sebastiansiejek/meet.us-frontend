{
  "name": "meet.us",
  "version": "1.0.0",
  "scripts": {
    "dev": "yarn graphql:codegen && next -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "analyze": "ANALYZE=true next build",
    "type-check": "tsc",
    "pre-commit": "lint-staged",
    "graphql:codegen": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen --config codegen.yml -r dotenv/config",
    "graphql:codegen:watch": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen --config codegen.yml -r dotenv/config --watch"
  },
  "engines": {
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@graphql-codegen/typescript-resolvers": "^2.3.2",
    "@next/bundle-analyzer": "^11.1.2",
    "@reduxjs/toolkit": "^1.6.2",
    "antd": "^4.16.13",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "graphql": "^15.6.1",
    "gsap": "^3.8.0",
    "hamburger-react": "^2.4.1",
    "js-cookie": "^3.0.1",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "next": "^11.1.2",
    "next-auth": "^4.0.6",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^8.9.0",
    "next-plugin-antd-less": "^1.4.4",
    "nprogress": "^0.2.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-leaflet": "^3.2.2",
    "react-loading-skeleton": "^3.0.1",
    "react-query": "^3.28.0",
    "react-redux": "^7.2.6",
    "styled-components": "^5.3.3",
    "yarn-upgrade-all": "^0.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@graphql-codegen/typescript-react-query": "^3.0.3",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/js-cookie": "^3.0.0",
    "@types/leaflet": "^1.7.6",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.5",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.10",
    "@types/styled-components": "^5.1.15",
    "autoprefixer": "^10.3.7",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.4",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.4.4"
  },
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "prettier --write",
